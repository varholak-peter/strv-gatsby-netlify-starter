{
  "name": "strv-gatsby-starter",
  "description": "STRV Gatsby Starter",
  "license": "MIT",
  "version": "0.0.1",
  "author": "Peter Varholak",
  "keywords": [
    "react",
    "gatsby",
    "netlify",
    "netlify-cms",
    "typescript",
    "styled-components",
    "starter"
  ],
  "scripts": {
    "apollo:download": "apollo service:download -c apollo.config.download.js",
    "apollo:gen": "rm -rf src/@generatedTypes && apollo client:codegen -c apollo.config.js --namespace GTypes --target typescript --outputFlat --tsFileExtension d.ts --useReadOnlyTypes src/@generatedTypes",
    "build": "npm run clean && gatsby build",
    "clean": "gatsby clean",
    "cypress": "cypress open",
    "dev": "npm run clean && gatsby develop",
    "format": "prettier --write",
    "lint": "eslint ./src ./cypress --ext .ts,.tsx,.js",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "serve": "gatsby serve",
    "serve:test": "gatsby serve -p 8000 & wait-on http://localhost:8000",
    "test": "npm run test:unit && npm run test:cypress",
    "test:cypress": "cypress run",
    "test:dev": "jest --watch",
    "test:unit": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.19.23",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-emotion": "^4.1.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-netlify": "^2.1.33",
    "gatsby-plugin-netlify-cms": "^4.1.40",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-page-creator": "^2.1.40",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.3.16",
    "parcel-bundler": "^1.9.4",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@strv/eslint-config-react": "^3.1.1",
    "@strv/eslint-config-typescript": "^2.2.0",
    "@strv/stylelint-config-styled-components": "^1.1.1",
    "@types/jest": "^25.1.3",
    "@types/react-helmet": "^5.0.12",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/parser": "^2.21.0",
    "apollo": "^2.24.0",
    "cypress": "^4.1.0",
    "eslint": "^6.5.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.13.0",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  }
}
