{
  "name": "strv-gatsby-netlify-starter",
  "description": "STRV Starter using Gatsby & Netlify CMS",
  "version": "0.0.1",
  "author": "Peter Varholak",
  "dependencies": {
    "gatsby": "^2.13.31",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-page-creator": "^2.1.27",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.26",
    "gatsby-transformer-remark": "^2.6.9",
    "gatsby-transformer-sharp": "^2.1.9",
    "netlify-cms-app": "^2.9.6",
    "parcel-bundler": "^1.9.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^6.0.0-beta",
    "styled-components": "^4.4.0"
  },
  "keywords": [
    "react",
    "gatsby",
    "netlify",
    "netlify-cms",
    "typescript",
    "styled-components",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "apollo:download": "apollo service:download -c apollo.config.download.js",
    "apollo:gen": "apollo client:codegen -c apollo.config.js --namespace GTypes --target typescript --outputFlat --tsFileExtension d.ts src/@generatedTypes",
    "build": "npm run clean && gatsby build",
    "clean": "gatsby clean",
    "cypress": "cypress open",
    "dev": "npm run clean && gatsby develop",
    "format": "prettier --write",
    "lint": "eslint ./src ./cypress --ext .ts,.tsx,.js",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "serve": "gatsby serve",
    "serve:test": "gatsby serve -p 8000 & wait-on http://localhost:8000",
    "test": "npm run test:unit && npm run test:cypress",
    "test:cypress": "cypress run",
    "test:unit": "jest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@strv/eslint-config-react": "^3.0.3",
    "@strv/eslint-config-typescript": "^2.1.3",
    "@strv/stylelint-config-styled-components": "^1.1.1",
    "@types/jest": "^24.0.19",
    "@types/react-helmet": "^5.0.12",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/parser": "^2.4.0",
    "apollo": "^2.20.0",
    "cypress": "^3.4.1",
    "eslint": "^6.5.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-typescript": "^2.1.15",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.10.2",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^8.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  }
}
